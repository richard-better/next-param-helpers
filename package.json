{
  "name": "next-param-helpers",
  "version": "0.0.0",
  "description": "",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.js",
  "scripts": {
    "_postinstall": "husky install",
    "prepublishOnly": "pinst --disable",
    "postpublish": "pinst --enable",
    "build": "node tools/cleanup && tsc -p config/tsconfig.build.json",
    "clean": "node tools/cleanup",
    "package": "pnpm run build && pnpm pack",
    "test": "jest --no-cache --runInBand",
    "test:cov": "jest --coverage --no-cache --runInBand",
    "addscope": "node tools/packagejson name @richard-better/next-param-helpers"
  },
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "dist"
  ],
  "keywords": [
    "example",
    "typescript",
    "npm",
    "package",
    "sample",
    "boilerplate",
    "starter",
    "demo",
    "cjs",
    "esm",
    "umd"
  ],
  "author": "Richard B <git@richardb.me>",
  "license": "MIT",
  "homepage": "https://github.com/richard-better/next-param-helpers",
  "repository": {
    "type": "git",
    "url": "git@github.com:richard-better/next-param-helpers.git"
  },
  "bugs": {
    "url": "https://github.com/richard-better/next-param-helpers/issues"
  },
  "dependencies": {
    "ts-toolbelt": "^9.6.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^13.1.0",
    "@commitlint/config-conventional": "^13.1.0",
    "@tsconfig/next": "^1.0.3",
    "@types/jest": "^27.0.1",
    "@typescript-eslint/eslint-plugin": "^4.31.1",
    "@typescript-eslint/parser": "^4.31.1",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "husky": "^7.0.2",
    "jest": "^27.2.0",
    "pinst": "^2.1.6",
    "prettier": "^2.4.0",
    "ts-jest": "^27.0.5",
    "typescript": "^4.8.4"
  }
}
